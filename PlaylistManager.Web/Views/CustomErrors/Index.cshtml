@model IEnumerable<PlaylistManager.Models.CustomException>

<table class="table">
    <tr style="text-align: justify; position: relative; right: 10em; left: 0em;" class="text-center">
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.StackTrace)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.HelpLink)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.Source)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.HResult)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.CustomMessage)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.CustomStackTrace)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.CustomInnerMessage)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.CustomInnerStackTrace)
        </th>
        <th style="text-align: center">
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StackTrace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HelpLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Source)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomMessage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomStackTrace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomInnerMessage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomInnerStackTrace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreated)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id })
        </td>
    </tr>
}

</table>
